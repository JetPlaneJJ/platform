          "groupBy": "location_type",
          "labels": {
            "culture": "Arts & Culture",
            "parks": "Parks & Recreation",
            "streets": "Streets & Sidewalks",
            "health": "Health & Wellness",
            "safety": "Safety & Environment",
            "other": "Other"
          },
          "layout": {
            "start": 1,
            "end": 7
          }
        },
        {
          "header": "Demographics",
          "type": "freeBarChart",
          "groupBy": "private-ethnicity",
          "groupValue": ["get-count"],
          "tooltipFormat": "count-percent",
          "xAxisLabel": "Ethnicity",
          "yAxisLabel": "Count",
          "annotation": "*race/ethinicity may not add up to 100% because of multiple choices",
          "labels": {
            "white": "White",
            "indian-alaskan": "American Indian/Alaskan Native",
            "asian": "Asian",
            "black": "Black or African American",
            "hispanic": "Hispanic or Latinx",
            "hawaiian-pacific": "Native Hawaiian or Pacific Islander",
            "other-ethnicity": "Other ethnicity"
          },
          "layout": {
            "start": 7,
            "end": 13
          }
        }
      ]
    },
    {
      "header": "PB Durham Projects",
      "isExportable": true,
      "datasetSlug": "pbdurham-projects",
      "widgets": [
        {
          "header": "Overview",
          "type": "statSummary",
          "rows": [
            {
              "type": "placeCount",
              "label": "Total projects"
            }
          ],
          "layout": {
            "start": 1,
            "end": 4
          }
        },
        {
          "header": "Engagement",
          "type": "lineChart",
          "groupBy": "created_datetime",
          "xAxisLabel": "Date",
          "yAxisLabel": "Number of projects",
          "layout": {
            "start": 4,
            "end": 13
          }
        },
        {
          "header": "Projects by Ward",
          "type": "freeDonutChart",
          "groupBy": "ward",
          "labels": {
            "ward_1": "Ward 1",
            "ward_2": "Ward 2",
            "ward_3": "Ward 3",
            "citywide": "Citywide"
          },
          "layout": {
            "start": 1,
            "end": 5
          }
        },
        {
          "header": "Budget by Ward (published proposals)",
          "type": "freeBarChart",
          "filter": ["!=", ["get-val", "private"], true],
          "groupBy": "ward",
          "groupValue": ["get-sum", "staff_project_budget"],
          "tooltipFormat": "currency",
          "labelFormat": "currency",
          "xAxisLabel": "Wards",
          "labels": {
            "ward_1": "Ward 1",
            "ward_2": "Ward 2",
            "ward_3": "Ward 3",
            "citywide": "Citywide"
          },
          "layout": {
            "start": 5,
            "end": 13
          }
        }
      ]
